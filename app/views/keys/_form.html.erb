<%= form_for(@key, url: yield(:form_url)) do |f| %>
  <%= render 'shared/error_messages', object: @key %>

  <%= f.label :name %>
  <%= f.text_field :name, class: 'form-control' %>

  <%= f.label :value %>
  <%= f.text_field :value, class: 'form-control' %>

  <%= f.submit yield(:button_text), class: "btn btn-primary" %>
<% end %>
